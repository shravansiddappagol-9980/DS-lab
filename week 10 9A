#include <stdio.h>

int queue[20], front = -1, rear = -1;
int visited[20], n, adj[20][20];

void bfs(int v)
{
    int i;
    visited[v] = 1;
    queue[++rear] = v;

    while (front != rear)
    {
        v = queue[++front];
        printf("%d ", v);

        for (i = 1; i <= n; i++)
        {
            if (adj[v][i] == 1 && visited[i] == 0)
            {
                visited[i] = 1;
                queue[++rear] = i;
            }
        }
    }
}

int main()
{
    int i, j, start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++)
            scanf("%d", &adj[i][j]);

    for (i = 1; i <= n; i++)
        visited[i] = 0;

    printf("Enter starting vertex: ");
    scanf("%d", &start);

    printf("BFS Traversal: ");
    bfs(start);

    return 0;
}
