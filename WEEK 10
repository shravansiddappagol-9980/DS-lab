#include <stdio.h>
#define SIZE 100

int ht[SIZE];

void insert(int key, int m)
{
    int index;
    index = key % m;

    while (ht[index] != -1)
    {
        index = (index + 1) % m;
    }
    ht[index] = key;
}

void display(int m)
{
    int i;
    printf("\nHash Table:\n");
    for (i = 0; i < m; i++)
    {
        if (ht[i] != -1)
            printf("Address %d : %d\n", i, ht[i]);
        else
            printf("Address %d : Empty\n", i);
    }
}                       

int main()
{
    int n, m, key, i;

    printf("Enter number of employee records: ");
    scanf("%d", &n);

    printf("Enter size of hash table (m): ");
    scanf("%d", &m);

    for (i = 0; i < m; i++)
        ht[i] = -1;

    printf("Enter %d four-digit employee keys:\n", n);
    for (i = 0; i < n; i++)
    {
        scanf("%d", &key);
        insert(key, m);
    }

    display(m);
    return 0;
}
